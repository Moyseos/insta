

<div class="profile-page">
<div class="upload-container">
<form class="upload-container-form" enctype="multipart/form-data" method="post" action ="/home">
	<% if (typeof error !== "undefined") { %>
		<p class="upload-error">
			<%- error %>
		</p>
	<% } %>
	<input class="uploader-input" type="file" name="file"/>
	<input class="uploader-description-input" method="post" type="text" name="description" placeholder="Descripe Your Image">
	<button class="upload-button">Upload</button>
</form>
</div>
</div>
